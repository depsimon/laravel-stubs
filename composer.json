{
    "name": "depsimon/laravel-stubs",
    "description": "Opinionated Laravel stubs",
    "keywords": [
        "depsimon",
        "gitscape",
        "laravel-stubs"
    ],
    "homepage": "https://github.com/depsimon/laravel-stubs",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Depelchin",
            "email": "simon@gitscape.com",
            "homepage": "https://gitscape.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^7.2|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Depsimon\\Stubs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depsimon\\Stubs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depsimon\\Stubs\\StubsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
